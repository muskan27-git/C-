// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;


    bool isprime(int n)
    {
        if(n<=0)
        {
            return false;
        }
        else
        {
            for(int i=2;i<=n;i++)
            {
                if(n%i==0)
                {
                    return false;
                }
                else
                {
                    return true;
                }
            }
        }
    }
    int main()
    {
      isprime(10) ? cout << " true\n" : cout << " false\n";
}
